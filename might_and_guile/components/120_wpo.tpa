//PROFICIENCIES FOR MOD CLERICS_____________________________________________________
//
COPY_EXISTING ~kitlist.2da~ ~override~
	COUNT_2DA_ROWS ~9~ "rows"
	PATCH_IF ( rows > 31 ) BEGIN
		FOR ( index = 32 ; index < rows ; index = index + 1 ) BEGIN
			READ_2DA_ENTRY %index% 5 9 modclab
			READ_2DA_ENTRY %index% 8 9 modclass
			DEFINE_ASSOCIATIVE_ARRAY d5_cleric_weaps BEGIN "%modclab%" => "%modclass%" END
		END
	END
BUT_ONLY
ACTION_PHP_EACH d5_cleric_weaps AS coo => qoo BEGIN
	ACTION_IF (%qoo% = 5) AND (FILE_EXISTS_IN_GAME ~%coo%.2da~) THEN BEGIN
		COPY_EXISTING ~weapprof.2da~ ~override~
			COUNT_2DA_COLS num_cols
			READ_2DA_ENTRIES_NOW ~kits_weapprof~ (num_cols - 1)
			FOR (col = 3; col < (num_cols - 1); col += 1) BEGIN
				READ_2DA_ENTRY_FORMER ~kits_weapprof~ 0 col modkit
				PATCH_IF (~%modkit%~ STRING_EQUAL_CASE ~%coo%~) BEGIN
					SET_2DA_ENTRY_LATER ~kits_weapprof~ 24 (col + 1) 1
					SET_2DA_ENTRY_LATER ~kits_weapprof~ 31 (col + 1) 1
					SET_2DA_ENTRY_LATER ~kits_weapprof~ 32 (col + 1) 1
