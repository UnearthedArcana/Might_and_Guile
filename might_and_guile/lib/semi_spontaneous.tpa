	PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~D5_DIV_1_7~) BEGIN
	  SET fake_sham_slots_1_7 = %row%
	END
	PATCH_IF (~%stat%~ STRING_EQUAL_CASE ~D5_DIV=1_7~) BEGIN
